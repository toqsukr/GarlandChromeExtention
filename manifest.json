{
  "manifest_version": 3,
  "name": "New Year Garland",
  "version": "1.0",
  "description": "This is a small New Year's garland project for chrome",
  "permissions": ["activeTab"],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["script.js"],
      "css": ["styles.css"]
    }
  ]
}
